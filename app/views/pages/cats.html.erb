<style media="screen">
  .cat{
    width:300px;
    height: 300px;
    display: none;
  }
</style>
<div class="article">
  <h1>Cats!</h1>
  <p>
    <% for i in 1..3 do %>
      <%= image_tag "cats/cat#{i}.jpg", class: "cat", id: "cat#{i}"%>
    <% end %>
  </p>
  <p>
    <%= button_tag "Previous", id: "prev" %>
    <%= button_tag "Next", id: "next" %>
  </p>
</div>
